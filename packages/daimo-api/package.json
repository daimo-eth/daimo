{
  "name": "@daimo/api",
  "version": "0.1.0",
  "license": "GPL-3.0-or-later",
  "description": "Daimo API for account creation, search, etc",
  "main": "src/index.ts",
  "scripts": {
    "dev": "OTEL_SERVICE_NAME=daimo-api ts-node-dev -r ./src/server/tracing.ts src/server/server.ts",
    "start": "OTEL_SERVICE_NAME=daimo-api node --max-old-space-size=7168 -- ../../node_modules/.bin/ts-node -r ./src/server/tracing.ts src/server/server.ts",
    "test": "tsc --noEmit && DAIMO_DOMAIN=example.com tape -r ts-node/register/transpile-only test/**/*.test.ts",
    "lint": "npm run lint:deps && npm run lint:style",
    "lint:deps": "npx depcheck --ignores @tsconfig/node20,@types/tape,ts-node",
    "lint:style": "eslint . --max-warnings=0"
  },
  "repository": "https://github.com/daimo-eth/daimo.git",
  "dependencies": {
    "@coinbase/cbpay-js": "^1.8.0",
    "@daimo/bulk": "^0.3.1",
    "@daimo/common": "*",
    "@daimo/contract": "*",
    "@daimo/userop": "*",
    "@honeycombio/opentelemetry-node": "^0.6.0",
    "@opentelemetry/api": "^1.4.0",
    "@opentelemetry/auto-instrumentations-node": "^0.39.3",
    "@trpc/server": "^10.43.2",
    "@types/cors": "^2.8.16",
    "@types/geoip-lite": "^1.4.2",
    "await-lock": "^2.2.2",
    "cors": "^2.8.5",
    "cron": "^3.1.4",
    "expo-server-sdk": "^3.7.0",
    "geoip-lite": "^1.4.7",
    "libhoney": "^4.1.0",
    "pg": "^8.11.0",
    "userop": "^0.3.2",
    "viem": "^1.19.15",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.0",
    "@types/pg": "^8.10.2",
    "@types/tape": "^5.6.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.44.0",
    "tape": "^5.6.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.2.2"
  }
}
