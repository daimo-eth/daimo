{
  "cli": {
    "version": ">= 3.9.2"
  },
  "build": {
    "sim": {
      "node": "20.3.0",
      "ios": {
        "buildConfiguration": "Debug",
        "resourceClass": "large",
        "simulator": true
      },
      "env": {
        "NEXT_PUBLIC_DOMAIN": "daimo.xyz"
      }
    },
    "dev": {
      "distribution": "internal",
      "resourceClass": "large",
      "node": "20.3.0",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      },
      "env": {
        "NEXT_PUBLIC_DOMAIN": "daimo.xyz"
      }
    },
    "stage": {
      "resourceClass": "large",
      "autoIncrement": true,
      "node": "20.3.0",
      "env": {
        "DAIMO_APP_API_URL": "https://daimo-api-prod.onrender.com",
        "NEXT_PUBLIC_DOMAIN": "daimo.xyz"
      }
    },
    "prod": {
      "autoIncrement": true,
      "resourceClass": "large"
    }
  },
  "submit": {
    "stage": {
      "ios": {
        "ascAppId": "6459700343",
        "companyName": "Daimo, Inc"
      },
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    },
    "prod": {}
  }
}
